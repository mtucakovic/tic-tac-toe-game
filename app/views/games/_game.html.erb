<div class="board">
  <div class="row">
    <div class="cell"><%= data[:game][:board][0] || 0 %></div>
    <div class="cell"><%= data[:game][:board][1] || 1 %></div>
    <div class="cell"><%= data[:game][:board][2] || 2 %></div>
  </div>
  <div class="row">
    <div class="cell"><%= data[:game][:board][3] || 3 %></div>
    <div class="cell"><%= data[:game][:board][4] || 4 %></div>
    <div class="cell"><%= data[:game][:board][5] || 5 %></div>
  </div>
  <div class="row">
    <div class="cell"><%= data[:game][:board][6] || 6 %></div>
    <div class="cell"><%= data[:game][:board][7] || 7 %></div>
    <div class="cell"><%= data[:game][:board][8] || 8 %></div>
  </div>
</div>